# Google Cloud Configuration
GOOGLE_GENAI_USE_VERTEXAI=true
GOOGLE_CLOUD_PROJECT=your-project-id
GOOGLE_CLOUD_LOCATION=us-central1
GOOGLE_CLOUD_STORAGE_BUCKET=your-bucket-name

# BigQuery Configuration
BIGQUERY_DATASET_BRONZE=marketing_raw
BIGQUERY_DATASET_SILVER=marketing_staging
BIGQUERY_DATASET_GOLD=marketing_marts

# Vertex AI Model Endpoints
VERTEX_AI_ENDPOINT_LEAD_SCORING=projects/PROJECT_ID/locations/LOCATION/endpoints/ENDPOINT_ID
VERTEX_AI_ENDPOINT_CHURN=projects/PROJECT_ID/locations/LOCATION/endpoints/ENDPOINT_ID
VERTEX_AI_ENDPOINT_CLV=projects/PROJECT_ID/locations/LOCATION/endpoints/ENDPOINT_ID

# Gemini Configuration
GEMINI_MODEL=gemini-2.0-flash-exp
GEMINI_TEMPERATURE=0.7
GEMINI_MAX_TOKENS=2048

# Marketing Platform APIs
GA360_PROPERTY_ID=your-property-id
DV360_ADVERTISER_ID=your-advertiser-id
SA360_ADVERTISER_ID=your-sa360-advertiser-id

# Agent Configuration
AGENT_LOG_LEVEL=INFO
AGENT_MAX_ITERATIONS=10
AGENT_TIMEOUT_SECONDS=300

# Performance Targets
API_LATENCY_TARGET_MS=100
BIGQUERY_QUERY_TIMEOUT_SECONDS=30
MODEL_PREDICTION_TIMEOUT_MS=100
